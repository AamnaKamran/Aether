<http://www.semanticweb.org/szm/megascan-assets-ontology> rdf:type owl:Ontology .

#################################################################
#    Object Properties
#################################################################

###  http://www.semanticweb.org/szm/megascan-assets-ontology#environmentHasBiome
:environmentHasBiome rdf:type owl:ObjectProperty ;
                     rdfs:subPropertyOf owl:topObjectProperty ;
                     rdfs:domain :Environment ;
                     rdfs:range :Biome .


###  http://www.semanticweb.org/szm/megascan-assets-ontology#environmentHasRegion
:environmentHasRegion rdf:type owl:ObjectProperty ;
                      rdfs:domain :Environment ;
                      rdfs:range :Region .


###  https://dbpedia.org/ontology/location
<https://dbpedia.org/ontology/location> rdf:type owl:ObjectProperty ;
                                        rdfs:domain :Environment ;
                                        rdfs:range :Region .


#################################################################
#    Data properties
#################################################################

###  http://www.semanticweb.org/szm/megascan-assets-ontology#assetAge
:assetAge rdf:type owl:DatatypeProperty ;
          rdfs:subPropertyOf :assetProperty ;
          rdfs:domain :Asset ;
          rdfs:comment """(check if this is a valid property)
could have values: baby, young, old, etc..""" .


###  http://www.semanticweb.org/szm/megascan-assets-ontology#assetAverageColor
:assetAverageColor rdf:type owl:DatatypeProperty ;
                   rdfs:domain :Asset ;
                   rdfs:range xsd:string ;
                   rdfs:comment "Asset Average Color must be a valid hex code" .


###  http://www.semanticweb.org/szm/megascan-assets-ontology#assetColor
:assetColor rdf:type owl:DatatypeProperty ;
            rdfs:subPropertyOf :assetProperty ;
            rdfs:domain :Asset ;
            rdfs:range xsd:string .


###  http://www.semanticweb.org/szm/megascan-assets-ontology#assetID
:assetID rdf:type owl:DatatypeProperty ;
         rdfs:domain :Asset ;
         rdfs:range xsd:string ;
         rdfs:comment "Asstet ID should be unique + alphanumeric" .


###  http://www.semanticweb.org/szm/megascan-assets-ontology#assetName
:assetName rdf:type owl:DatatypeProperty ;
           rdfs:domain :Asset ;
           rdfs:range xsd:string .


###  http://www.semanticweb.org/szm/megascan-assets-ontology#assetOrientation
:assetOrientation rdf:type owl:DatatypeProperty ;
                  rdfs:subPropertyOf :assetProperty ;
                  rdfs:domain :Asset .


###  http://www.semanticweb.org/szm/megascan-assets-ontology#assetProperty
:assetProperty rdf:type owl:DatatypeProperty ;
               rdfs:subPropertyOf owl:topDataProperty ;
               rdfs:comment "includes asset properties under \"properties\" key in asset .json file" .


###  http://www.semanticweb.org/szm/megascan-assets-ontology#assetSize
:assetSize rdf:type owl:DatatypeProperty ;
           rdfs:subPropertyOf :assetProperty ;
           rdfs:domain :Asset ;
           rdfs:range xsd:string ;
           rdfs:comment """Asset size can be large, small, tiny, ...
Fixed sizes (to be searched)""" .


###  http://www.semanticweb.org/szm/megascan-assets-ontology#assetTag
:assetTag rdf:type owl:DatatypeProperty ;
          rdfs:domain :Asset ;
          rdfs:range xsd:string .


###  http://www.semanticweb.org/szm/megascan-assets-ontology#categoryLabel
:categoryLabel rdf:type owl:DatatypeProperty ;
               rdfs:domain :Category ;
               rdfs:range xsd:string .


###  http://www.w3.org/2000/01/rdf-schema#label
rdfs:label rdf:type owl:DatatypeProperty ;
           rdfs:range xsd:string .


#################################################################
#    Classes
#################################################################

###  http://www.semanticweb.org/szm/megascan-assets-ontology#Age
:Age rdf:type owl:Class ;
     rdfs:subClassOf :Property .


###  http://www.semanticweb.org/szm/megascan-assets-ontology#Asset
:Asset rdf:type owl:Class .


###  http://www.semanticweb.org/szm/megascan-assets-ontology#Biome
:Biome rdf:type owl:Class .


###  http://www.semanticweb.org/szm/megascan-assets-ontology#Category
:Category rdf:type owl:Class .


###  http://www.semanticweb.org/szm/megascan-assets-ontology#Color
:Color rdf:type owl:Class ;
       rdfs:subClassOf :Property .


###  http://www.semanticweb.org/szm/megascan-assets-ontology#Environment
:Environment rdf:type owl:Class .


###  http://www.semanticweb.org/szm/megascan-assets-ontology#MainLevelCategory
:MainLevelCategory rdf:type owl:Class ;
                   rdfs:subClassOf :Category .


###  http://www.semanticweb.org/szm/megascan-assets-ontology#Orientation
:Orientation rdf:type owl:Class ;
             rdfs:subClassOf :Property .


###  http://www.semanticweb.org/szm/megascan-assets-ontology#Property
:Property rdf:type owl:Class .


###  http://www.semanticweb.org/szm/megascan-assets-ontology#Region
:Region rdf:type owl:Class .


###  http://www.semanticweb.org/szm/megascan-assets-ontology#Size
:Size rdf:type owl:Class ;
      rdfs:subClassOf :Property .


###  http://www.semanticweb.org/szm/megascan-assets-ontology#SubLevelCategory
:SubLevelCategory rdf:type owl:Class ;
                  rdfs:subClassOf :Category .


###  http://www.semanticweb.org/szm/megascan-assets-ontology#TopLevelCategory
:TopLevelCategory rdf:type owl:Class ;
                  rdfs:subClassOf :Category .


#################################################################
#    General axioms
#################################################################

[ rdf:type owl:AllDisjointClasses ;
  owl:members ( :Asset
                :Biome
                :Category
                :Environment
                :Property
                :Region
              )
] .


